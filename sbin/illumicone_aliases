alias testpattern='killall -USR2 patternController'

alias reinitpc='killall -USR1 patternController'
alias disablepc='sudo systemctl disable patternController.service'
alias enablepc='sudo systemctl enable patternController.service'
alias startpc='sudo systemctl start patternController.service'
alias stoppc='sudo systemctl stop patternController.service'
alias reloadpc='sudo systemctl reload patternController.service'

alias reinitwr='killall widgetRcvr'
