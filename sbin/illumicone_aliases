alias enablewr='sudo systemctl enable widgetRcvr.service'
alias startwr='sudo systemctl start widgetRcvr.service'
alias stopwr='sudo systemctl stop widgetRcvr.service'
alias reloadwr='sudo systemctl reload widgetRcvr.service'

